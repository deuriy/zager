extends layouts/base

block content
  .Banner.Banner-compare(style="background-image: url('img/bg_banner_guitars.webp')")
    .Container
      .Breadcrumbs.Banner_breadcrumbs
        ul.Breadcrumbs_list
          li.Breadcrumbs_item
            a(href="/").Breadcrumbs_link Home
          li.Breadcrumbs_item
            a(href="/guitars.html").Breadcrumbs_link Guitars
          li.Breadcrumbs_item
            span.Breadcrumbs_text Compare models
      .Banner_textWrapper
        h1.Banner_title Class aptent taciti sociosqu ad
        a(href="#").BtnYellow.BtnYellow-video.Banner_btn Watch the video
