    .Container
      .AccessoriesSection_wrapper
        h2.SectionTitle.AccessoriesSection_title Don’t forget your accessories
        .AccessoriesSwiper.AccessoriesSection_swiper.swiper-container
          .swiper-wrapper
            .swiper-slide.AccessoriesSwiper_slide
              .AccessoryCard.AccessoryCard-swiper
                .AccessoryCard_imgWrapper
                  img(src="img/accessory_img_2.webp", alt="Accessory").AccessoryCard_img
                  span.Tag.Tag-accessoryCard.AccessoryCard_tag On sale
                .AccessoryCard_textWrapper
                  h3.AccessoryCard_title Zager Professional Capo
                  .AccessoryCard_price $39.00
                  a(href="#").BtnYellow.BtnYellow-accessoryCard.AccessoryCard_btn.AccessoryCard_btn-addToCart Add to cart
                  a(href="#").BtnOutline.BtnOutline-lightBeigeBg.BtnOutline-darkText.BtnOutline-accessoryCard.AccessoryCard_btn.hidden-xs view details
            .swiper-slide.AccessoriesSwiper_slide
              .AccessoryCard.AccessoryCard-swiper
                .AccessoryCard_imgWrapper
                  img(src="img/accessory_img_3.webp", alt="Accessory").AccessoryCard_img
                .AccessoryCard_textWrapper
