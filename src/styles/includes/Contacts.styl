.Contacts
  &_list
    display grid
    grid-template-columns repeat(2, auto)
    list-style none
    column-gap 25px
  
  &_item
    position relative
    
    &::after
      content '|'
      position absolute
      right -14px
      font-weight 600
      font-family $sourceSansPro
      color $grey
    
    &:last-child::after
      display none